{
  "family": "nodejs-user-management-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "nodejs-user-management",
      "image": "698445805226.dkr.ecr.us-east-2.amazonaws.com/nodejsusermanagement:1.0",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::698445805226:role/ecsTaskExecutionRole"
}
