{
  "family": "nodejs-holiday-bot-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "nodejs-holiday-bot",
      "image": "698445805226.dkr.ecr.us-east-2.amazonaws.com/nodejsholidaybot:1.0",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "KHITROV_HOLIDAY_BOT_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-2:698445805226:parameter/KHITROV_HOLIDAY_BOT_TOKEN"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::698445805226:role/ecsTaskExecutionRole"
}
